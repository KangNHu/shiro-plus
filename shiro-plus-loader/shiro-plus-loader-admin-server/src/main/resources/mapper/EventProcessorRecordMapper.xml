<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.codingeasy.shiroplus.loader.admin.server.dao.EventProcessorRecordDao">

   <insert id="batchInsert">
       <choose>
           <when test="entities == null || entities.size() == 0">
               select 1
           </when>
           <otherwise>
               INSERT INTO sp_event_processor_record(host,event)VALUES
               <foreach collection="entities" item="entity" separator=",">
                   (
                   #{entity.host},
                   #{entity.event}
                   )
               </foreach>
           </otherwise>
       </choose>
   </insert>
</mapper>